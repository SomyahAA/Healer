<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="350dp"
        android:layout_height="550dp"
        android:layout_gravity="center"
        android:background="@drawable/dialog_border">


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/profileImage"
            android:layout_width="237dp"
            android:layout_height="189dp"
            android:layout_marginTop="28dp"
            android:src="@drawable/doc"
            app:civ_border_color="@color/white"
            app:civ_border_width="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.503"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/psyName"
            android:layout_width="296dp"
            android:layout_height="59dp"
            android:layout_marginBottom="100dp"
            android:text="Dr.Somyah Alsayed"
            android:textAlignment="center"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/imageButton2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.513"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/profileImage" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="309dp"
            android:layout_height="58dp"
            android:layout_marginTop="24dp"
            android:text="fgbsfgshgfhnthsthnthnh"
            android:textAlignment="center"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.536"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/psyName" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="68dp"
            android:layout_height="61dp"
            android:layout_marginStart="56dp"
            android:layout_marginEnd="19dp"
            android:layout_marginBottom="56dp"
            android:background="@drawable/ic_call_24"
            android:contentDescription="make call Button"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageButton3"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="69dp"
            android:layout_height="57dp"
            android:layout_marginEnd="56dp"
            android:layout_marginBottom="56dp"
            android:background="@drawable/ic_chat_24"
            android:contentDescription="Make a Chat Button"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="70dp"
            android:layout_height="59dp"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="56dp"
            android:background="@drawable/ic_video"
            android:contentDescription="Maje a video call Button"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageButton2" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>